    "eslint": "^7.30.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "rimraf": "^3.0.2"
  }
